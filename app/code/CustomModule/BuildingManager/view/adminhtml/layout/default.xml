<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <link src="CustomModule_BuildingManager::js/custom.js"/>
    </head>
    <body>
        <referenceContainer name="report" remove="true"/>
        <referenceContainer name="header">

        <block class="Magento\Backend\Block\Page\Header" name="logo" before="-">
            <arguments>
                <argument name="show_part" xsi:type="string">logo</argument>
                <argument name="edition" translate="true" xsi:type="string">Community Edition</argument>
                <argument name="logo_image_src" xsi:type="string">CustomModule_BuildingManager::images/home.svg</argument>
            </arguments>
        </block>
    </referenceContainer>
    </body>
</page>